# Copy to .env and adjust as needed

# Match your host user to avoid permission issues with bind mounts
UID=your_uid
GID=your_gid

# Database
POSTGRES_USER=your_postgres_user
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=your_postgres_db

# Ghidra
MCPO_API_KEY=your_mcpo_api_key
GHIDRA_INSTALL_DIR=your_ghidra_install_dir
GHIDRA_MAXMEM=your_ghidra_maxmem
